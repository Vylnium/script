-- Scripts:

local function QPNOE_fake_script() -- ESP.KeybindClose 
	local script = Instance.new('LocalScript', ESP)

	local FrameObject = script.Parent.MainFrame
	local Open = false
	
	
	local PositionClosed = UDim2.new(0,0,0,0)
	local PositionOpen = UDim2.new(0.375, 0,0.302, 0)
	local UserInputService = game:GetService("UserInputService")
	
	UserInputService.InputBegan:Connect(function(KeyCode)
		if KeyCode.KeyCode == Enum.KeyCode.LeftControl then
			if Open then
				FrameObject:TweenSize((PositionClosed), "Out", "Bounce")
				FrameObject.SecondaryFrame.Pages.HomePage.Profile.Visible = false
				wait(1)
				FrameObject.Visible = false
				Open = false
			else
				
				FrameObject.Visible = true
				wait(0.2)
				FrameObject:TweenSize((PositionOpen), "Out", "Back")
				Open = true
				FrameObject.SecondaryFrame.Pages.HomePage.Profile.Visible = true
			end
		end
	end)
end
coroutine.wrap(QPNOE_fake_script)()
local function HUIERX_fake_script() -- PlayerEsp.PlayerEsp 
	local script = Instance.new('LocalScript', PlayerEsp)

	local Esp = "Players" -- change to folder of what u want an esp of
	local Btn = script.Parent
	local Tgl = false
	local Loc = game:GetService(Esp)
	-- name text gui
	-- Instances:
	local BillboardGui = Instance.new("BillboardGui")
	local TextLabel = Instance.new("TextLabel")
	--Properties:
	BillboardGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
	BillboardGui.Active = true
	BillboardGui.AlwaysOnTop = true
	BillboardGui.LightInfluence = 1
	BillboardGui.Size = UDim2.new(0, 150, 0, 35)
	BillboardGui.StudsOffset = Vector3.new(0, 2, 0)
	
	TextLabel.Parent = BillboardGui
	TextLabel.BackgroundColor3 = Color3.new(1, 1, 1)
	TextLabel.BackgroundTransparency = 1
	TextLabel.Size = UDim2.new(1, 0, 1, 0)
	TextLabel.Font = Enum.Font.SourceSans
	TextLabel.Text = "Object"
	TextLabel.TextColor3 = Color3.new(1, 0, 0)
	TextLabel.TextScaled = true
	TextLabel.TextSize = 14
	TextLabel.TextStrokeTransparency = 0
	TextLabel.TextWrapped = true
	-- esp button fctn
	Btn.MouseButton1Click:Connect(function()
		if Tgl == false then
			Tgl = true
			while Tgl == true do
				for i,v in pairs(Loc:GetChildren()) do
					if v ~= game.Players.LocalPlayer then
						local High = Instance.new("Highlight")
						local BillC = BillboardGui:Clone()
						local Char = v.Character
						High.FillTransparency = 0.1
						High.OutlineColor = Color3.new(255, 0, 255)
						High.Parent = Char
						BillC.Parent = Char.Head
						BillC.Adornee = Char.Head
						BillC.TextLabel.Text = v.Name .. " Health: " .. v.Character.Humanoid.Health
					end
				end
				wait(3)
				for i,v in pairs(Loc:GetChildren()) do
					local char = v.Character
					if char:FindFirstChildWhichIsA("Highlight") and char.Head:FindFirstChildWhichIsA("BillboardGui"):Destroy() then
						char:FindFirstChildWhichIsA("Highlight"):Destroy()
						char.Head.FindFirstChildWhichIsA("BillboardGui"):Destroy()
					end
				end
			end
		elseif Tgl == true then
			Tgl = false
			for i,v in pairs(Loc:GetChildren()) do
				local char = v.Character
				if char:FindFirstChildWhichIsA("Highlight") and char.Head:FindFirstChildWhichIsA("BillboardGui"):Destroy() then
				char:FindFirstChildWhichIsA("Highlight"):Destroy()
					char.Head:FindFirstChildWhichIsA("BillboardGui"):Destroy()
				else
				end
			end
		end
	end)
end
coroutine.wrap(HUIERX_fake_script)()
local function WVLV_fake_script() -- Profile.LocalScript 
	local script = Instance.new('LocalScript', Profile)

	local Players = game:GetService("Players")
	
	local player = Players.LocalPlayer
	local userId = player.UserId
	local thumbType = Enum.ThumbnailType.HeadShot
	local thumbSize = Enum.ThumbnailSize.Size60x60
	local content, isReady = Players:GetUserThumbnailAsync(userId, thumbType, thumbSize)
	
	local imageLabel = script.Parent
	imageLabel.Image = content
	imageLabel.Size = UDim2.new(0,60,0,60)
	
end
coroutine.wrap(WVLV_fake_script)()
local function UCVCV_fake_script() -- Username.LocalScript 
	local script = Instance.new('LocalScript', Username)

	local player = game.Players.LocalPlayer
	
	if player then
		script.Parent.Text = "Hello "..player.Name.."!"
	end
end
coroutine.wrap(UCVCV_fake_script)()
local function NCTCYI_fake_script() -- GroupLink.LocalScript 
	local script = Instance.new('LocalScript', GroupLink)

	local plr = game.Players.LocalPlayer
	local btn = script.Parent
	local startGui = game:GetService("StarterGui")
	
	local Players = game:GetService("Players")
	
	local player = Players.LocalPlayer
	local userId = player.UserId
	local thumbType = Enum.ThumbnailType.HeadShot
	local thumbSize = Enum.ThumbnailSize.Size60x60
	local content, isReady = Players:GetUserThumbnailAsync(userId, thumbType, thumbSize)
	
	
	
	
	
	
	btn.MouseButton1Up:Connect(function()
		setclipboard("https://www.roblox.com/groups/11965777/xla-voidz#!/about")
		wait(0.2)
		toclipboard("https://www.roblox.com/groups/11965777/xla-voidz#!/about")
		wait(0.3)
		startGui:SetCore("SendNotification",{
			Title = " Group link copied to Clipboard!",
			Text = "Close Gui and re-execute after you have joined the group.",
			Icon = content,
			Duration = 5,
			Button1 = "OK",
		})
	end)
end
coroutine.wrap(NCTCYI_fake_script)()
local function REZSQB_fake_script() -- EggEsp.EggEsp 
	local script = Instance.new('LocalScript', EggEsp)

	local Esp = "Eggs" -- change to folder of what u want an esp of
	local Btn = script.Parent
	local Tgl = false
	local Loc = game.Workspace:FindFirstChild(Esp)
	-- Name Text Gui
	-- Instances:
	local BillboardGui = Instance.new("BillboardGui")
	local TextLabel = Instance.new("TextLabel")
	--Properties:
	BillboardGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
	BillboardGui.Active = true
	BillboardGui.AlwaysOnTop = true
	BillboardGui.LightInfluence = 1
	BillboardGui.Size = UDim2.new(0, 150, 0, 35)
	BillboardGui.StudsOffset = Vector3.new(0, 2, 0)
	
	TextLabel.Parent = BillboardGui
	TextLabel.BackgroundColor3 = Color3.new(1, 1, 1)
	TextLabel.BackgroundTransparency = 1
	TextLabel.Size = UDim2.new(1, 0, 1, 0)
	TextLabel.Font = Enum.Font.SourceSans
	TextLabel.Text = "Object"
	TextLabel.TextColor3 = Color3.new(1, 0, 0)
	TextLabel.TextScaled = true
	TextLabel.TextSize = 14
	TextLabel.TextStrokeTransparency = 0
	TextLabel.TextWrapped = true
	-- ESP Button Function
	Btn.MouseButton1Click:Connect(function()
		if Tgl == false then
			Tgl = true
			while Tgl == true do
				for i,v in pairs(Loc:GetChildren()) do
					local Dist = math.round((game.Players.LocalPlayer.Character.HumanoidRootPart.Position - v.Handle.Position).magnitude)
					local High = Instance.new("Highlight")
					local BillC = BillboardGui:Clone()
					High.Parent = v
					High.FillTransparency = 0.1
					High.OutlineColor = Color3.new(255, 0, 255)
					BillC.Parent =  v
					BillC.Adornee = v
					BillC.TextLabel.Text = "EGG,  Distance: " .. Dist
				end
				wait(3)
				for i,v in pairs(Loc:GetDescendants()) do
					if v:IsA("Highlight") or v:IsA("BillboardGui") then
						v:Destroy()
					end
				end
			end
		elseif Tgl == true then
			Tgl = false
			for i,v in pairs(Loc:GetChildren()) do
				for n,part in pairs(v:GetChildren()) do
					if part:IsA("Highlight") or part:IsA("BillboardGui") then
						part:Destroy()
					end
				end
			end
		end
	end)
end
coroutine.wrap(REZSQB_fake_script)()
local function XIRUA_fake_script() -- MainFrame.CategoryOpeningScript 
	local script = Instance.new('LocalScript', MainFrame)

	local category = script.Parent.SecondaryFrame.Categorys
	local esp = script.Parent.SecondaryFrame.Pages.ESPS
	local espbutton = script.Parent.SecondaryFrame.Categorys.ListStuff.ESP
	local misc = script.Parent.SecondaryFrame.Categorys.ListStuff.MiscButton
	local miscs = script.Parent.SecondaryFrame.Pages.misc
	local home = script.Parent.SecondaryFrame.Pages.HomePage
	local homebutton = script.Parent.SecondaryFrame.Categorys.ListStuff.Home
	
	
	espbutton.MouseButton1Click:Connect(function()
		esp.Visible = not esp.Visible
		if esp.Visible == true then
			miscs.Visible = false
			if home.Visible == true then
				home.Visible = false
			end
		end
	end)
	misc.MouseButton1Click:Connect(function()
		miscs.Visible = not miscs.Visible
		if miscs.Visible == true then
			esp.Visible = false
			if home.Visible == true then
				home.Visible = false
			end
		end
	end)
	
	homebutton.MouseButton1Click:Connect(function()
		home.Visible = true
		if home.Visible == true then
			miscs.Visible = false
			if home.Visible == true then
				esp.Visible = false
			end
		end
	end)
	
	
	
end
coroutine.wrap(XIRUA_fake_script)()
